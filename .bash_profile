# .bash_profile

# Neofetch and startx on login
[ -z $DISPLAY ] && [ $(tty) = /dev/tty1 ] && neofetch; startx

# Export
export EDITOR='micro'
export VISUAL='micro'
export TERM='urxvt'
export BROWSER='firefox'
